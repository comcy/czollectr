<!-- views/partials/header.ejs -->
<nav class="header navbar navbar-default" role="navigation">
    <div class="container-fluid">
        <div class="navbar-header">
            <a class="navbar-brand" href="#">
                <span class="glyphicon glyphicon glyphicon-cloud-download"></span>
                <%=
        site.title %>
            </a>

            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="<%= site.baseHref %>" class="btn">
                            <%= site.homelabel %> </a>
                    </li>
                    <ul class="nav navbar-nav">
                        <li>
                            <a href="<%= site.baseHref %>" class="btn">
                                <%= site.homelabel %> </a>
                        </li>
                        <% navigation.keys().forEach( ( key ) => { %>
                        <li>
                            <%= key %>
                            <% navigation.get(key).forEach( ( item ) => { %>
                            <a href="/<%= item.link %>/<%= item.name %>">
                                <%= item.displayName %>
                            </a>
                            <% }) %>
                        </li>
                        <% }) %>
                    </ul>
                </ul>
            </div>
        </div>
    </div>
</nav>